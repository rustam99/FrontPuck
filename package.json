{
  "name": "frontpuck",
  "version": "1.0.0",
  "description": "base front pack",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "webpack serve  --open --config ./webpack/webpack.dev.conf.js",
    "build": "eslint ./src --fix --color && webpack --config ./webpack/webpack.build.conf.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "eslint ./src --fix --color && git add .",
    "h:init": "husky init",
    "prepare": "husky install"
  },
  "private": true,
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "keywords": [
    "webpack",
    "husky",
    "eslint",
    "babel"
  ],
  "author": "rustam",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.1.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.2.0",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "postcss-loader": "^5.2.0",
    "sass-loader": "^11.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.26.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "jquery": "^3.6.0",
    "normalize.css": "^8.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rustam99/FrontPuck"
  }
}
